{
    "manifest_version": 3,
    "name": "URL Extractor",
    "version": "4.0",
    "action": {
      "default_popup": "popup.html"
    },
      "icons": {
        "16": "icon_16.png",
        "32": "icon_32.png",
        "128": "icon_128.png"
      },
    
    "permissions": ["activeTab",
                    "tabs",
                  "scripting",
                "storage"
              ],
 "background":{
        "service_worker":"background.js"
              }
  }
  